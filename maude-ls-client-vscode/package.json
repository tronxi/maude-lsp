{
  "name": "maude-ls-client-vscode",
  "displayName": "maude-ls-client-vscode",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:maude"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "maude",
        "aliases": [
          "Maude",
          "maude"
        ],
        "extensions": [
          ".maude"
        ]
      }
    ],
    "commands": [
      {
        "command": "maude-ls-client-vscode.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@types/mz": "0.0.32"
  },
  "dependencies": {
    "mz": "2.7.0",
    "vscode-languageserver-types": "3.14.0",
    "vscode-languageclient": "7.0.0"
  }
}
